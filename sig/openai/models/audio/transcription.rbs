module OpenAI
  module Models
    module Audio
      type transcription = { text: String }

      class Transcription < OpenAI::BaseModel
        attr_accessor text: String

        def initialize:
          (text: String) -> void
          | (
            ?OpenAI::Models::Audio::transcription | OpenAI::BaseModel data
          ) -> void

        def to_hash: -> OpenAI::Models::Audio::transcription
      end
    end
  end
end
