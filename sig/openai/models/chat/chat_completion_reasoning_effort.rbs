module OpenAI
  module Models
    
    class ChatCompletionReasoningEffort = Chat::ChatCompletionReasoningEffort

    module Chat
      type chat_completion_reasoning_effort = :low | :medium | :high

      class ChatCompletionReasoningEffort < OpenAI::Enum
        LOW: :low
        MEDIUM: :medium
        HIGH: :high

        def self.values: -> ::Array[OpenAI::Models::Chat::chat_completion_reasoning_effort]
      end
    end
  end
end
