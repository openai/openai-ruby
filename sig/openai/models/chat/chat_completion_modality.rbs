module OpenAI
  module Models
    module ChatCompletionModality = Chat::ChatCompletionModality

    module Chat
      type chat_completion_modality = :text | :audio

      module ChatCompletionModality
        extend OpenAI::Enum

        TEXT: :text
        AUDIO: :audio

        def self?.values: -> ::Array[OpenAI::Models::Chat::chat_completion_modality]
      end
    end
  end
end
