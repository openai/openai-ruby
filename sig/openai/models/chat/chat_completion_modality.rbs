module OpenAI
  module Models
    
    class ChatCompletionModality = Chat::ChatCompletionModality

    module Chat
      type chat_completion_modality = :text | :audio

      class ChatCompletionModality < OpenAI::Enum
        TEXT: :text
        AUDIO: :audio

        def self.values: -> ::Array[OpenAI::Models::Chat::chat_completion_modality]
      end
    end
  end
end
