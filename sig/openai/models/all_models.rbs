module OpenAI
  module Models
    type all_models =
      String
      | OpenAI::Models::chat_model
      | OpenAI::Models::AllModels::responses_only_model

    module AllModels
      extend OpenAI::Internal::Type::Union

      type responses_only_model =
        :"o1-pro"
        | :"o1-pro-2025-03-19"
        | :"computer-use-preview"
        | :"computer-use-preview-2025-03-11"

      module ResponsesOnlyModel
        extend OpenAI::Internal::Type::Enum

        O1_PRO: :"o1-pro"
        O1_PRO_2025_03_19: :"o1-pro-2025-03-19"
        COMPUTER_USE_PREVIEW: :"computer-use-preview"
        COMPUTER_USE_PREVIEW_2025_03_11: :"computer-use-preview-2025-03-11"

        def self?.values: -> ::Array[OpenAI::Models::AllModels::responses_only_model]
      end

      def self?.variants: -> [String, OpenAI::Models::chat_model, OpenAI::Models::AllModels::responses_only_model]
    end
  end
end
