module OpenAI
  module Models
    type reasoning_effort = :none | :minimal | :low | :medium | :high

    module ReasoningEffort
      extend OpenAI::Internal::Type::Enum

      NONE: :none
      MINIMAL: :minimal
      LOW: :low
      MEDIUM: :medium
      HIGH: :high

      def self?.values: -> ::Array[OpenAI::Models::reasoning_effort]
    end
  end
end
