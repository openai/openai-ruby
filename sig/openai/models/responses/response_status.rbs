module OpenAI
  module Models
    module Responses
      type response_status = :completed | :failed | :in_progress | :incomplete

      class ResponseStatus < OpenAI::Enum
        COMPLETED: :completed
        FAILED: :failed
        IN_PROGRESS: :in_progress
        INCOMPLETE: :incomplete

        def self.values: -> ::Array[OpenAI::Models::Responses::response_status]
      end
    end
  end
end
