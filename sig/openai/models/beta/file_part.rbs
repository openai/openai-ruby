module OpenAI
  module Models
    module Beta
      type file_part =
        {
          id: String,
          mime_type: String?,
          name: String?,
          type: :file,
          upload_url: String?
        }

      class FilePart < OpenAI::Internal::Type::BaseModel
        attr_accessor id: String

        attr_accessor mime_type: String?

        attr_accessor name: String?

        attr_accessor type: :file

        attr_accessor upload_url: String?

        def initialize: (
          id: String,
          mime_type: String?,
          name: String?,
          upload_url: String?,
          ?type: :file
        ) -> void

        def to_hash: -> {
          id: String,
          mime_type: String?,
          name: String?,
          type: :file,
          upload_url: String?
        }
      end
    end
  end
end
